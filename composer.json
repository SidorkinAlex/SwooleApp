{
    "name": "sidalex/swoole-app",
    "description": "a package for organizing http services on a swoole server",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Sidalex\\SwooleApp\\": "src/"
        }
    },
    "authors": [
        {
            "name": "SidAlex",
            "email": "palach.orl@mail.ru"
        }
    ],
    "require": {
        "yurunsoft/guzzle-swoole": "^2.2",
        "haydenpierce/class-finder": "^0.5.3",
        "oittaa/uuid": "^1.9",
        "psr/log": "^3.0"
    },
    "config": {
        "allow-plugins": {
            "yurunsoft/composer-include-files": false,
            "yurunsoft/guzzle-swoole": true
        }
    },
    "require-dev": {
        "phpstan/phpstan": "^1.10",
        "phpunit/phpunit": "^10.5"
    }
}
